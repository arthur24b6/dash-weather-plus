{
	"manifest_version": 2,
	
	"name": "Dash Weather+",
	"author": "Garrett Downs",
	"description": "Beautiful weather app build entirely with Enyo2.",
	"version": "1.1.0",
	"icons": {
		"256": "icon-256.png",
		"128": "icon-128.png",
		"64": "icon-64.png",
		"32": "icon-32.png",
		"48": "icon-48.png",
		"24": "icon-24.png"
	},
	"app": {
		"background": {
			"scripts": ["background.js"]
	    }
	},
	"permissions": [
		"<all_urls>",
		"geolocation",
		"storage"
	]
}